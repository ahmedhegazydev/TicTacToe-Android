<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ahmed.tictactoe"
    android:versionCode="4"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme.NoTitle" >
        <activity
            android:name="com.ahmed.human.PlayHuman"
            android:noHistory="true" >
            
            
        </activity>
        <activity
            android:name="com.ahmed.machine.PlayMachine"
            android:noHistory="true" >
        </activity>
        <activity
            android:name="com.ahmed.choose.ComputerOrHuman"
            android:label="@string/title_activity_computer_or_human" >
        </activity>
        <activity
            android:name="com.ahmed.human.PlayHumanLevel2"
            android:label="@string/title_activity_play_human_level2"
            android:noHistory="true" >
        </activity>
        <activity
            android:name="com.ahmed.human.HumanLevels"
            android:label="@string/title_activity_human_levels" >
        </activity>
        
        
        <activity
            android:name="com.ahmed.machine.MachineLevels"
            android:label="@string/title_activity_machine_levels" >
        </activity>
        
        <activity
            android:name="com.ahmed.machine.PlayMachineLevel2"
            android:label="@string/title_activity_play_machine_level2"
            android:noHistory="true" >
        </activity>
        
        <activity
            android:name="com.ahmed.win.WinGame"
            android:label="@string/title_activity_win_game" >
        </activity>
        
        
        <activity
            android:name="com.ahmed.start.StartGame"
            android:label="@string/title_activity_start_game" >
           
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
             
            
        </activity>
        
        
    </application>

</manifest>
